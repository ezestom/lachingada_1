---
import Layout from "../../layouts/Layout.astro";
import { EmblaCarousel } from "../EmblaCarousel/EmblaCarousel.jsx";
import Container from "./Container.astro";
---

<Layout title="">
	<section
		class="w-full h-full bg-red-50 z-10 sticky top-0 flex items-center justify-center flex-col min-h-screen"
		id="gallery">
		<Container>
			<article
				class="flex flex-col mt-16 md:mt-0 md:flex-row md:justify-between justify-center gap-10 w-full">
				<div class="w-full">
					<EmblaCarousel client:visible />
				</div>
			</article>
		</Container>
	</section>
</Layout>
